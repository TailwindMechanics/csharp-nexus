{
    "workbench.colorTheme": "Monokai Night",
    "omnisharp.sdkVersion": "latest",
    "editor.codeLens": false,
    "editor.formatOnSave": true,
    "[csharp]": {
        "editor.defaultFormatter": "ms-dotnettools.csharp"
    },
    "explorer.compactFolders": false,
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
    "editor.detectIndentation": false,
    "explorer.fileNesting.patterns": {
        "root": ".env, .gitignore, appsettings.json, autoCommentPath.js, Dockerfile, dotnet_cruiser.csproj, dotnet_cruiser.sln, Program.cs, project-hierarchy.txt, README.md, rootGrouper.js"
    },
    "material-icon-theme.folders.associations": {
        "Properties": "Utils",
        "Persona": "Admin",
        "GptChat": "Command",
        "AvatarGen": "Robot",
        "Graph": "Shader",
        "Agents": "Robot",
        "Obj": "Packages",
        "Health": "Tools",
        "Hubs": "Public",
        "Drawer": "Class",
        "Agent": "Robot",
        "Dev": "Tools",
        "Root": "Base",
        "DispatchRouter": "Routes",
        "Bulletin": "Messages",
        "Envars": "Secure",
        "LogkeepFrigate": "Log",
        "Lifetime": "Generator",
        "ShipsInfo": "Delta",
    },
    "material-icon-theme.files.associations": {
        "root": "Tree"
    },
    "project-hierarchy-explorer.ignorePatterns": [
        ".git",
        "env",
        "bin",
        "obj",
        ".vscode"
    ]
}